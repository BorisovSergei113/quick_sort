#include <iostream>
#include <sstream>
using namespace std;

void quick_Sort(int *mas,int length){ 
    int size = length;
    int k =0;
    int c=mas[length/2];
    do {
        while (mas[k]<c) k++;
        while (mas[length]>c) length--;
        if(k<=length){
            swap(mas[k], mas[length]);
            k++;
            length--;
        }
    }
    while (k<=length);                    
    if(length>0) quick_Sort(mas,length); 
    if(size>k) quick_Sort(mas+k, size-k);   

}

int main(){
     int length =0;

    string str;
    getline(cin,str);
    istringstream stream1(str);
    if(!(stream1>>length)){
        cout<<"An error has occuried while reading input data."<<endl;
        exit(0);
    }

    int *mas=new int [length];

    string mas_str;
    getline(cin,mas_str);
    istringstream stream (mas_str);
    for(int i=0;i<length;i++){
        if(!(stream>>mas[i])){
            cout<<"An error has occuried while reading input data."<<endl;
            delete [] mas;
            exit(0);
        }
    }

    quick_Sort(mas, length-1);

    for(int i=0;i<length;i++){
        cout<<mas[i]<<" ";
    }
    cout<<endl;

    delete [] mas;
    return 0;
}
